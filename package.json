{
  "name": "my-express-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "seed": "tsnd --transpile-only --ignore-watch node_modules src/utils/seed.ts",
    "dev": "tsnd --exit-child --respawn --transpile-only --ignore-watch node_modules src/index.ts",
    "build": "npx prisma migrate deploy && rimraf dist && tsc --declaration",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint --ext js,ts .",
    "lint:fix": "eslint --ext js,ts . --fix",
    "prepare": "husky install",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "prettier --write .",
      "eslint --ext js,ts . --fix"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.1.1",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.1",
    "celebrate": "^15.0.1",
    "cors": "^2.8.5",
    "cross-fetch": "^4.0.0",
    "dotenv": "^16.0.3",
    "eosjs": "^22.1.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.4",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.11.11",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "husky": "^8.0.2",
    "jest": "^29.6.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.0",
    "prisma": "^5.1.1",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.3"
  }
}
